                    <h1 class="heading-2 mb-10">Your Cart</h1>
                    <div class="d-flex justify-content-between">
                        <h6 class="text-body">There are <span class="text-brand"><%= cart.products.length %></span> products in your cart</h6>
                        <h6 class="text-body"><a href="#" class="text-muted"><i class="fi-rs-trash mr-5"></i>Clear Cart</a></h6>
                    </div>
                    <div class="divider-2 mb-30"></div>
                    <div class="table-responsive shopping-summery">
                        <table class="table table-wishlist">
                            <thead>
                                <tr class="main-heading">
                                    <th class="start pl-30" scope="col">Product</th>
                                    <th scope="col"></th>
                                    <th scope="col">Unit Price</th>
                                    <th scope="col">Quantity</th>
                                    <th scope="col" class="end">Subtotal</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% let cartTotal = 0; %>
                                <% cart.products.forEach(function(product) { %>
                                <tr class="pt-30">
                                    <td class="image product-thumbnail pt-40">
                                        <img src="/user_assets/imgs/shop/<%= product.productId.images[0] %>" alt="#">
                                    </td>
                                    <td class="product-des product-name">
                                        <h6 class="mb-5">
                                            <a class="product-name mb-10 text-heading" href="/product/<%= product.productId._id %>">
                                                <%= product.productId.name %>
                                            </a>
                                        </h6>
                                        <div class="product-rate-cover">
                                            <div class="product-rate d-inline-block">
                                                <div class="product-rating" style="width: <%= product.productId.rating %>%;"></div>
                                            </div>
                                            <span class="font-small ml-5 text-muted"> (<%= (product.productId.rating / 20).toFixed(1) %>)</span>
                                        </div>
                                    </td>
                                    <td class="price" data-title="Price">
                                        <h4 class="text-body">$<%= product.productId.price %></h4>
                                    </td>
                                    <td class="text-center detail-info" data-title="Stock">
                                        <div class="detail-extralink">
                                            <div class="detail-qty border radius">
                                                <a href="#" class="qty-down" data-id="<%= product.productId._id %>"><i class="fi-rs-angle-small-down"></i></a>
                                                <span class="qty-val" id="qty-<%= product.productId._id %>"><%= product.quantity %></span>
                                                <a href="#" class="qty-up" data-id="<%= product.productId._id %>"><i class="fi-rs-angle-small-up"></i></a>
                                            </div>
                                        </div>
                                    </td>
                                    <td class="text-right" data-title="Cart">
                                        <h4 class="text-body item-total" id="item-total-<%= product.productId._id %>" data-price="<%= product.productId.price %>">$<%= (product.productId.price * product.quantity).toFixed(2) %></h4>
                                    </td>
                                    <td class="action" data-title="Remove">
                                        <a href="/cart/remove/<%= product.productId._id %>" class="text-muted"><i class="fi-rs-trash"></i></a>
                                    </td>
                                </tr>
                                <% cartTotal += product.productId.price * product.quantity; %>
                                <% }) %>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="6" class="text-right">
                                        <h4 class="text-body" id="cart-total">Total: $<%= cartTotal.toFixed(2) %></h4>
                                    </td>
                                </tr>
                            </tfoot>
                            
                            <script>
                                document.querySelectorAll('.qty-up, .qty-down').forEach(button => {
        button.addEventListener('click', function(event) {
            event.preventDefault();
            const isIncrease = this.classList.contains('qty-up');
            const productId = this.getAttribute('data-id');
            const qtyElement = document.getElementById('qty-' + productId);
            let quantity = parseInt(qtyElement.textContent);

            // Update quantity
            if (isIncrease) {
                quantity++;
            } else if (quantity > 1) {
                quantity--;
            }

            qtyElement.textContent = quantity;

            // Update item total
            const itemTotalElement = document.getElementById('item-total-' + productId);
            const itemPrice = parseFloat(itemTotalElement.getAttribute('data-price'));
            const itemTotal = (itemPrice * quantity).toFixed(2);
            itemTotalElement.textContent = '$' + itemTotal;

            // Update cart total
            updateCartTotal();
        });
    });

    function updateCartTotal() {
        let cartTotal = 0;
        document.querySelectorAll('.item-total').forEach(itemTotalElement => {
            const itemTotal = parseFloat(itemTotalElement.textContent.replace('$', ''));
            if (!isNaN(itemTotal)) {
                cartTotal += itemTotal;
            }
        });
        const cartSubtotalElement = document.getElementById('cart-subtotal');
        const cartGrandTotalElement = document.getElementById('cart-grand-total');
        
        cartSubtotalElement.textContent = '$' + cartTotal.toFixed(2);
        cartGrandTotalElement.textContent = '$' + cartTotal.toFixed(2);
        
        document.getElementById('cart-total').textContent = 'Total: $' + cartTotal.toFixed(2);
    }
                            </script>
                            
                            
